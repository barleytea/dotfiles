{
  "vscode-neovim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.navigateLeft"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.navigateDown"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.navigateUp"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.navigateRight"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<S-h>"],
      "commands": ["workbench.action.previousEditor"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<S-l>"],
      "commands": ["workbench.action.nextEditor"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<Esc>"],
      "commands": [
        "vscode-neovim.send",
        "search.action.cancel"
      ],
      "when": "neovim.mode == 'normal' && editorTextFocus"
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeActiveEditor"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.view.scm"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.findInFiles"],
      "when": "neovim.mode == 'normal'"
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.startFindReplaceAction"],
      "when": "neovim.mode == 'normal'"
    }
  ],
  "vscode-neovim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-h>"],
      "commands": ["cursorLeft"],
      "when": "neovim.mode == 'insert' && editorTextFocus"
    },
    {
      "before": ["<C-j>"],
      "commands": ["cursorDown"],
      "when": "neovim.mode == 'insert' && editorTextFocus"
    },
    {
      "before": ["<C-k>"],
      "commands": ["cursorUp"],
      "when": "neovim.mode == 'insert' && editorTextFocus"
    },
    {
      "before": ["<C-l>"],
      "commands": ["cursorRight"],
      "when": "neovim.mode == 'insert' && editorTextFocus"
    },
    {
      "before": ["<C-s>"],
      "commands": ["workbench.action.files.save"],
      "when": "neovim.mode == 'insert' && editorTextFocus"
    }
  ],
  "vscode-neovim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"],
      "when": "neovim.mode == 'visual'"
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"],
      "when": "neovim.mode == 'visual'"
    }
  ]
}

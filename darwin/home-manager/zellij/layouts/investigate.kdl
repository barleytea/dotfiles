layout {
    default_tab_template {
        pane size=1 borderless=true {
            plugin location="compact-bar"
        }
        children
    }

    tab focus=true {
        pane split_direction="vertical" {
            pane size="33%" name="log"
            pane size="33%" name="log2"
            pane split_direction="horizontal" {
                pane name="investigate"
                pane name="notes"
            }
        }
    }
}
